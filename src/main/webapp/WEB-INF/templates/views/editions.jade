- attributes("title") = "List of Editions"
- attributes("headline") = "Editions"
-@ val editions: List[com.chicagof1.model.Edition]

h1 Editions
ul(class='editions list-unstyled')
  - for(e <- editions) {
  li
    .edition
      .printId
        a(href='editions/#{e.date}')= e.printId
      .detail
        h3= e.winner
        p= e.results(0).formattedTime
        p= e.date
  - }

- render("analytics.ssp")